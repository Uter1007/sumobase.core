{
  "db": {
    "debug": false,
    "uri": "//mydbUrl"
  },
  "mail": {
    "domain": "sandbox19446.mailgun.org"
  },
  "log": {
    "morganLogFormat": ":method :url :status :response-time ms - :res[content-length]",
    "streamLogLevel": "debug",
    "winstonOptions": {
      "exitOnError": false,
      "transports": [
        {
          "colorize": false,
          "filename": "./logs/all-logs.log",
          "handleExceptions": true,
          "json": true,
          "level": "info"
        },
        {
          "colorize": true,
          "handleExceptions": true,
          "json": false,
          "level": "debug"
        },
        {
          "handleExceptions": true,
          "level": "info",
          "timestamp": true
        }
      ]
    }
  }
}
